{
  "name": "@voxelize/client",
  "version": "1.0.12",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "umd": "dist/voxelize.min.js",
  "license": "MIT",
  "scripts": {
    "dev": "npm-run-all --parallel \"dev:rs\" \"compile -w\" \"types -w --preserveWatchOutput\"",
    "dev:rs": "cross-env RUST_BACKTRACE=full cargo watch -w server -x \"build --release\"",
    "demo": "npm-run-all --parallel \"demo:ts\" \"demo:rs\"",
    "demo:ts": "cd examples/client && yarn && yarn demo",
    "demo:rs": "cross-env RUST_BACKTRACE=full cargo watch -w examples/server -w server -x \"run --release --example demo\"",
    "build": "yarn run clean && yarn run compile && yarn run types",
    "clean": "rimraf -rf ./dist",
    "compile": "rollup -c rollup.config.js",
    "types": "tsc --emitDeclarationOnly --outDir ./dist -p tsconfig.build.json --declaration --allowJs",
    "prepublishOnly": "yarn run build",
    "lint": "eslint --fix --ext js,ts,tsx .",
    "proto:ts": "pbts -o client/protocol/index.d.ts client/protocol/index.js",
    "proto:js": "pbjs -t static-module -w commonjs -o client/protocol/index.js messages.proto",
    "proto": "yarn run proto:js && yarn run proto:ts",
    "docs": "yarn run docs:start",
    "docs:start": "cd docs && yarn && yarn run start",
    "docs:build": "cd docs && yarn && yarn run build",
    "docs:deploy": "cd docs && yarn && yarn run deploy"
  },
  "peerDependencies": {
    "@voxelize/voxel-physics-engine": "^1.0.10",
    "three": "^0.141.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^4.0.0",
    "@swc/core": "^1.2.163",
    "@tweakpane/core": "^1.0.8",
    "@types/domurl": "^0.0.7",
    "@types/gl-vec3": "^1.1.1",
    "@types/jest": "^27.4.1",
    "@types/mousetrap": "^1.6.9",
    "@types/ndarray": "^1.0.11",
    "@types/pako": "^1.0.3",
    "@types/simple-peer": "^9.11.4",
    "@types/three": "^0.137.0",
    "@types/tweezer.js": "^1.4.3",
    "@types/typedarray-pool": "^1.1.2",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "peer": "^0.6.1",
    "prettier": "^2.5.1",
    "protobufjs": "^6.11.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.67.0",
    "rollup-plugin-glslify": "^1.2.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-swc3": "^0.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "rollup-plugin-web-worker-loader": "^1.6.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@voxelize/voxel-physics-engine": "^1.0.10",
    "deepmerge": "^4.2.2",
    "domurl": "^2.3.4",
    "events": "^3.3.0",
    "fflate": "^0.7.3",
    "gl-vec3": "^1.1.3",
    "mousetrap": "^1.6.5",
    "ndarray": "^1.0.19",
    "pako": "^2.0.4",
    "postprocessing": "^6.26.3",
    "simple-peer": "^9.11.0",
    "socket.io-client": "^4.4.1",
    "three-spritetext": "^1.6.5",
    "tweakpane": "^3.0.7",
    "tweezer.js": "^1.5.0",
    "typedarray-pool": "^1.2.0"
  }
}
