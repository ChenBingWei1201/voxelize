{
  "name": "@voxelize/client",
  "version": "1.2.17",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "umd": "dist/voxelize.min.js",
  "scripts": {
    "dev": "npm-run-all --parallel \"compile -w\" \"types --preserveWatchOutput\"",
    "clean": "rimraf ./dist",
    "compile": "rollup -c rollup.config.js",
    "build": "yarn run clean && yarn run compile && yarn run types",
    "types": "tsc --emitDeclarationOnly --outDir ./dist -p ./tsconfig.json --declaration --allowJs",
    "prepublishOnly": "yarn run build"
  },
  "peerDependencies": {
    "three": "^0.142.0"
  },
  "devDependencies": {
    "@tweakpane/core": "^1.0.8",
    "@types/gl-vec3": "^1.1.1",
    "@types/mousetrap": "^1.6.9",
    "@types/ndarray": "^1.0.11",
    "@types/three": "^0.141.0"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^18.6.4",
    "@voxelize/aabb": "^1.0.0",
    "@voxelize/physics-engine": "^1.1.24",
    "@voxelize/raycast": "^1.0.5",
    "@voxelize/transport": "*",
    "deepmerge": "^4.2.2",
    "domurl": "^2.3.4",
    "events": "^3.3.0",
    "fflate": "^0.7.3",
    "gl-vec3": "^1.1.3",
    "mousetrap": "^1.6.5",
    "ndarray": "^1.0.19",
    "postprocessing": "^6.26.3",
    "socket.io-client": "^4.4.1",
    "three-spritetext": "^1.6.5",
    "tweakpane": "^3.0.7"
  }
}
