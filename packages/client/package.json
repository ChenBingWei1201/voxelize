{
  "name": "@voxelize/client",
  "version": "1.0.6",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "umd": "dist/voxelize.min.js",
  "license": "MIT",
  "scripts": {
    "dev": "npm-run-all --parallel \"compile -- -w\" \"types -- -w --preserveWatchOutput\"",
    "build": "yarn run clean && yarn run compile && yarn run types",
    "clean": "rimraf -rf ./dist",
    "compile": "rollup -c rollup.config.js",
    "types": "tsc --emitDeclarationOnly --outDir ./dist -p tsconfig.build.json --declaration --allowJs",
    "posttypes": "mv ./dist/src/* ./dist && rimraf ./dist/src",
    "prepublishOnly": "yarn run build"
  },
  "peerDependencies": {
    "three": "^0.137.5"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@tweakpane/core": "^1.0.8",
    "@types/domurl": "^0.0.7",
    "@types/mousetrap": "^1.6.9",
    "@types/pako": "^1.0.3",
    "@types/simple-peer": "^9.11.4",
    "@types/three": "^0.137.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.67.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-swc3": "^0.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "rollup-plugin-web-worker-loader": "^1.6.1"
  },
  "dependencies": {
    "@voxelize/common": "*",
    "deepmerge": "^4.2.2",
    "domurl": "^2.3.4",
    "mousetrap": "^1.6.5",
    "pako": "^2.0.4",
    "simple-peer": "^9.11.0",
    "socket.io-client": "^4.4.1",
    "three-spritetext": "^1.6.5",
    "tweakpane": "^3.0.7"
  }
}
