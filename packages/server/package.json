{
  "name": "@voxelize/server",
  "version": "1.0.12",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "npm-run-all --parallel \"compile -- -w\" \"types -- -w --preserveWatchOutput\"",
    "build": "yarn run clean && yarn run compile",
    "clean": "rimraf -rf ./dist",
    "compile": "yarn run types && rollup -c rollup.config.js",
    "types": "tsc --emitDeclarationOnly --outDir ./dist -p tsconfig.build.json --declaration --allowJs",
    "prepublishOnly": "yarn run build"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@swc/core": "^1.2.143",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.67.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-swc3": "^0.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "rollup-plugin-web-worker-loader": "^1.6.1"
  },
  "peerDependencies": {
    "@voxelize/common": "*"
  },
  "dependencies": {
    "@math.gl/core": "^3.5.7",
    "@types/ws": "^8.2.2",
    "cors": "^2.8.5",
    "deepmerge": "^4.2.2",
    "express": "^4.17.2",
    "peer": "^0.6.1",
    "socket.io": "^4.4.1",
    "uuid": "^8.3.2",
    "ws": "^8.4.2"
  }
}
